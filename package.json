{
  "name": "associum",
  "version": "0.2.0",
  "description": "A TypeScript library for efficient multi-key Maps.",
  "main": "distribution/source/associum.js",
  "types": "distribution/source/associum.d.ts",
  "type": "module",
  "keywords": [
    "multikey-map",
    "indexed-map",
    "index",
    "queryable-map",
    "data-structures",
    "typescript"
  ],
  "author": "Magnus Meseck",
  "license": "MIT",
  "scripts": {
    "build": "rm -r ./distribution || true && tsc",
    "patch": "npm run build && npm version patch && git push --tags && npm publish",
    "release": "npm run build && npm version minor && git push --tags && npm publish",
    "test": "tsc --noemit && node --no-warnings --experimental-transform-types test/test.ts"
  },
  "dependencies": {
    "identigenium": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elemental-mind/associum.git"
  },
  "homepage": "https://github.com/elemental-mind/associum#readme"
}
